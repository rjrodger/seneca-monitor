{
  "name": "seneca-monitor",
  "version": "0.3.0",
  "description": "seneca-monitor",
  "main": "monitor.js",
  "scripts": {
    "test": "lab -P test -t 75",
    "coveralls": "lab -s -P test -r lcov | coveralls",
    "coverage": "lab -v -P test -L -t 75 -r html > docs/coverage.html",
    "prettier": "prettier --write --no-semi --single-quote monitor.js test/*.js",
    "clean-npm": "rm -rf node_modules package-lock.json",
    "clean-yarn": "rm -rf node_modules yarn.lock",
    "repo-tag": "REPO_VERSION=`node -e \"console.log(require('./package').version)\"` && echo TAG: v$REPO_VERSION && git commit -a -m v$REPO_VERSION && git push && git tag v$REPO_VERSION && git push --tags;",
    "repo-publish": "npm test && npm run repo-tag && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjrodger/seneca-monitor.git"
  },
  "keywords": [
    "seneca",
    "microservice",
    "monitor"
  ],
  "author": "Richard Rodger richardrodger.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rjrodger/seneca-monitor/issues"
  },
  "homepage": "https://github.com/rjrodger/seneca-monitor#readme",
  "devDependencies": {
    "code": "5",
    "coveralls": "3",
    "lab": "18",
    "prettier": "1",
    "seneca": "senecajs/seneca",
    "seneca-repl": "1"
  },
  "dependencies": {
    "hapi": "17",
    "inert": "5"
  },
  "files": [
    "LICENSE",
    "README.md",
    "monitor.js",
    "www"
  ]
}
